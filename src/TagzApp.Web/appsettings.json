{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "providers": {
    "mastodon": {
      "BaseAddress": "https://mastodon.social",
      "Timeout": "00:03:00",
      "DefaultHeaders": {
      },
      "UseHttp2": true
    },
    "youtube": {
      "ApiKey": "my-random-key"
    }
  },
  "HttpPolicies": {
    "HttpCircuitBreaker": {
      "DurationOfBreak": "00:01:00",
      "ExceptionsAllowedBeforeBreaking": 20
    },
    "HttpRetry": {
      "BackoffPower": 2,
      "Count": 3
    }
  },
  "AllowedHosts": "*"
}
